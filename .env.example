# Telegram app credentials from https://my.telegram.org
TG_API_ID=123456
TG_API_HASH=your_api_hash

# String session generated via tools/generate_session.py
TG_SESSION_STRING=your_string_session

# Target chat to listen. Use numeric chat id (preferred) or public username.
TG_TARGET_CHAT=-1001234567890

# RabbitMQ connection for listener container
MQ_URL=amqp://guest:guest@rabbitmq:5672/
MQ_EXCHANGE=telegram.messages
MQ_QUEUE=telegram.messages.raw
MQ_ROUTING_KEY=telegram.message
MQ_PERSISTENT=true

SERVICE_NAME=telegram-userbot-listener

# RabbitMQ bootstrap credentials
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
